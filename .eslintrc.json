{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "globals": {},
  "rules": {
    "indent": ["error", 2],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/destructuring-assignment": 1,
    "react/prop-types": 1
  },
  "parserOptions": {
    "ecmaVersion": 8
  }
}
